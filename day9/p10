def string_both_ends(input_string):
	#start writing your code here
    if(len(input_string)<2):
        return -1
    else:
        s=""
        s=s+input_string[0]+input_string[1]+input_string[-1]+input_string[-2]
        return s
input_string="w3w"
print(string_both_ends(input_string))
