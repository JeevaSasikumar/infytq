def find_five_digit():
    n2=0
    n3=0
    n4=0
    n5=0
    for i in range(9,-1,-1):
        n2=int(i-2)
        n3=int(n2-2)
        n4=int(n3-2)
        n5=int(n3)
        if(n3+n4+n5==i and n2+n3+n4+n5+i==19):
            break
    s=str(i)+str(n2)+str(n3)+str(n4)+str(n5)
    print(s)

find_five_digit()
